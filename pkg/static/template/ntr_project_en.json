ALESCE(state,'') as state, COALESCE(zip_code,'') as zipcode, COALESCE(city,'') as city, COALESCE(street,'') as street, COALESCE(notes,'') as notes from project_addresses where code = @code"},"contact":{"default":"select id as id, code as pronumber, project_name as description, COALESCE(first_name,'') as firstname, COALESCE(surname,'') as surname, COALESCE(status,'') as status, COALESCE(phone,'') as phone, '' as fax, COALESCE(mobile,'') as mobil, COALESCE(email,'') as email, COALESCE(notes,'') as cont_notes from project_contacts where code = @code"},"trans":{"default":"select t.code as transnumber, t.trans_type as transtype, t.direction, t.trans_date as transdate, COALESCE(t.currency_code, '') as curr, COALESCE((select sum(amount) from item_view where trans_code = t.code),0) as amount, COALESCE(c.customer_name, '') as customer from trans_view t left join customer c on t.customer_code = c.code where t.code = @code"},"event":{"default":"select code as calnumber, tag_lst as tags, substr(cast(start_time as char(10)), 1, 10) as fromdate, substr(cast(start_time as char(16)), 12, 5) as fromtime, substr(cast(end_time as char(10)), 1, 10) as todate, substr(cast(end_time as char(16)), 12, 5) as totime, COALESCE(subject,'') as subject, COALESCE(place,'') as place, COALESCE(description,'') as description from project_events where code = @code","mssql":"select code as calnumber, tag_lst as tags, CONVERT(VARCHAR(10), start_time, 120) as fromdate, CONVERT(VARCHAR(5), start_time, 108) as fromtime, CONVERT(VARCHAR(10), end_time, 120) as todate, CONVERT(VARCHAR(5), end_time, 108) as totime, COALESCE(subject,'') as subject, COALESCE(place,'') as place, COALESCE(description,'') as description from project_events where code = @code"}},"data":{"logo":"data:image/jpg;base64,","labels":{"lb_additional_data":"Additional data","lb_address_details":"Address details","lb_amount":"Amount","lb_city":"City","lb_comment":"Comment","lb_contact_details":"Contact details","lb_currency":"Curr","lb_cust